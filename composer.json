    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Upmind\\DomainNameApiSdk\\": "src/",
            "Upmind\\DomainNameApiSdk\\SDK\\": "SDK/"
        }
    },
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-soap": "*",
        "ext-xml": "*",
        "wsdltophp/packagebase": "~5.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "wsdltophp/packagegenerator": "^4.1"
    }
}
